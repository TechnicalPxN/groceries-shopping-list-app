{
  "cli": {
    "version": ">= 13.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "default": {
      "resourceClass": "large"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "extends": "default"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "preview",
      "extends": "default"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "extends": "default"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "environment": "development",
      "extends": "default"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./PlayStoreServiceAccount.json",
        "track": "internal"
      }
    }
  }
}
